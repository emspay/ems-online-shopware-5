<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="emspay.service"
                 class="emspay\Components\Emspay\Service">
        </service>
        <service id="emspay.helper"
                 class="emspay\Components\Emspay\Helper">
        </service>
        <service id="emspay_events.subscriber.status_subscriber" class="emspay\Subscriber\StatusSubscriber">
                <tag name="shopware.event_subscriber"/>
        </service>
        <service id="emspay_events.subscriber.checkout_subscriber" class="emspay\Subscriber\CheckoutSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="emspay_events.subscriber.idealissuer_subscriber" class="emspay\Subscriber\IDealIssuerSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
    </services>
</container>
